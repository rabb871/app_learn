/* components/tab-bar/tab-bar.wxss */
.tab-bar {
  display: flex;
  justify-content: space-around;
  width: 100%;
  height: 98rpx;
  align-items: center;
  background-color: #f3e4b2;
  
}
.tab-item {
  font-size: 30rpx;
  color: #928f72;
  font-weight: 700;
}
.tab-item.active {
  color: #c95132;
  font-weight: 1100;
}